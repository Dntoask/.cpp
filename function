#include <iostream>

using namespace std;

void print_numbers(int x)
{
    for(int n = 1;n <= x;n++){
        cout << n << endl;
    }
}
int main()
{
    for(int n = 1;n < 6;n++){

        if(n = 3){
           cout << "when number is " << n << endl;
           print_numbers(n);
        }if(n = 5){
            cout << "when number is " << n << endl;
            print_numbers(n);
        }
}
    return 0;
}
