#include <iostream>

using namespace std;

int main()
{
    int arr[6] = {1, 3, 2, 8, 9, 7};
    int length = 6;

    int x = 2;
    cin >> x;

    bool b = false;
    for (int i = 0; i < length; i++){
        if (arr[i] == x) {
            b = true;
        }
    }

    if (b == true)
        cout << "Your number is in the array" << endl;
    else if (b == false)
        cout << "Your number is not in the array" << endl;

    return 0;
}
